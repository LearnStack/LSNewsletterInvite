GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/ChimpKit" "${PODS_ROOT}/Headers/Public/LSNewsletterInvite" "${PODS_ROOT}/Headers/Public/SVProgressHUD"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/ChimpKit" -isystem "${PODS_ROOT}/Headers/Public/LSNewsletterInvite" -isystem "${PODS_ROOT}/Headers/Public/SVProgressHUD"
OTHER_LDFLAGS = -ObjC -l"Pods-ChimpKit" -l"Pods-LSNewsletterInvite" -l"Pods-SVProgressHUD" -framework "QuartzCore"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods